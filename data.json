        {
          "name": "1.1",
          "title": "1.1 Theoretical Concepts",
          "description": "Mathematical and statistical foundations essential for modeling and solving problems using AI, including graph theory and the concept of vectorization.",
          "level": 3,
          "chapter":1,
          "children": [
            {"chapter":1,"name": "Black Box", "title": "Black Box", "description": "System or model whose internal workings are not visible or understandable to the user, only the input and output are known.", "level": 4},
            {"chapter":1,"name": "Causal Inference", "title": "Causal Inference", "description": "Process of determining the cause-and-effect relationship between variables.", "level": 4},
            {"chapter":1,"name": "Deterministic", "title": "Deterministic", "description": "System or process is one that, given a particular initial state, will always produce the same output or result, with no randomness or unpredictability involved.", "level": 4},
            {"chapter":1,"name": "Graph Theory", "title": "Graph Theory", "description": "Field of mathematics and computer science focusing on the properties of graphs, which are structures made up of vertices (or nodes) connected by edges.", "level": 4},
            {"chapter":1,"name": "Optimization Problem", "title": "Optimization Problem", "description": "Optimization problem in AI which involves finding the best solution from all feasible solutions, given a set of constraints and an objective to achieve or optimize.", "level": 4},
            {"chapter":1,"name": "Scalar", "title": "Scalar", "description": "Single numerical value, typically representing a quantity or magnitude in mathematical or computational models.", "level": 4},
            {"chapter":1,"name": "Stochastic", "title": "Stochastic", "description": "Systems or processes that are inherently random, involving variables that are subject to chance.", "level": 4},
            {"chapter":1,"name": "Structured Data", "title": "Structured Data", "description": "Information that is highly organized and formatted in a way that is easily searchable and accessible by computer systems, typically stored in databases.", "level": 4},
            {"chapter":1,"name": "Symbolic AI", "title": "Symbolic AI", "description": "Also known as Good Old-Fashioned AI (GOFAI), involves the manipulation of symbols to represent problems and compute solutions through rules.", "level": 4},
            {"chapter":1,"name": "Unstructured Data", "title": "Unstructured Data", "description": "Data that lacks a pre-defined format or organization, making it challenging to collect, process, and analyze using conventional database tools.", "level": 4},
            {"chapter":1,"name": "Vectorization", "title": "Vectorization", "description": "Process of converting non-numeric data into numeric format so that it can be used by ML algorithms.", "level": 4}
