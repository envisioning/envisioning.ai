        {
            "id": "Welcome",
            "title": "Envisioning.AI",
            "description": "Emerging vocabulary to explain AI",
            "parent_id": null,
            "level": 1,
            "chapter": null
        },
        {
            "id": "1",
            "title": "1. Foundations",
            "description": "Understanding the fundamental principles, theories, and mathematical underpinnings critical to AI development and research.",
            "parent_id": "Welcome",
            "level": 2,
            "chapter": 1
        },
        {
            "id": "1.1",
            "title": "1.1 Theoretical Concepts",
            "description": "Mathematical and statistical foundations essential for modeling and solving problems using AI, including graph theory and the concept of vectorization.",
            "parent_id": "1",
            "level": 3,
            "chapter": 1
        },
        {
            "id": "Black Box",
            "title": "Black Box",
            "description": "System or model whose internal workings are not visible or understandable to the user, only the input and output are known.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Causal Inference",
            "title": "Causal Inference",
            "description": "Process of determining the cause-and-effect relationship between variables.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Deterministic",
            "title": "Deterministic",
            "description": "System or process is one that, given a particular initial state, will always produce the same output or result, with no randomness or unpredictability involved.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Graph Theory",
            "title": "Graph Theory",
            "description": "Field of mathematics and computer science focusing on the properties of graphs, which are structures made up of vertices (or nodes) connected by edges.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Optimization Problem",
            "title": "Optimization Problem",
            "description": "Optimization problem in AI which involves finding the best solution from all feasible solutions, given a set of constraints and an objective to achieve or optimize.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Scalar",
            "title": "Scalar",
            "description": "Single numerical value, typically representing a quantity or magnitude in mathematical or computational models.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Stochastic",
            "title": "Stochastic",
            "description": "Systems or processes that are inherently random, involving variables that are subject to chance.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Structured Data",
            "title": "Structured Data",
            "description": "Information that is highly organized and formatted in a way that is easily searchable and accessible by computer systems, typically stored in databases.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Symbolic AI",
            "title": "Symbolic AI",
            "description": "Also known as Good Old-Fashioned AI (GOFAI), involves the manipulation of symbols to represent problems and compute solutions through rules.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Unstructured Data",
            "title": "Unstructured Data",
            "description": "Data that lacks a pre-defined format or organization, making it challenging to collect, process, and analyze using conventional database tools.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
        {
            "id": "Vectorization",
            "title": "Vectorization",
            "description": "Process of converting non-numeric data into numeric format so that it can be used by ML algorithms.",
            "parent_id": "1.1",
            "level": 4,
            "chapter": 1
        },
